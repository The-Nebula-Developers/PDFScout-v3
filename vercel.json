{
    "builds": [
        {
            "src":"api/main.py",
            "use":"@vercel/python",
            "engines": {"node": "18.x"},
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9"}
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ]
}
