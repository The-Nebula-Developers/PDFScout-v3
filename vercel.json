{
    "builds": [
        {
            "src":"api/main.py",
            "use":"@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9"}
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ],
    [
      "engines": {
        "node": "18.x"
      }
    ]

}
